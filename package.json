{
  "name": "@camelot/web-globe",
  "private": true,
  "version": "1.1.0",
  "description": "A simple web component",
  "main": "camelot-globe",
  "module": "camelot-globe",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:prod": "rollup -c",
    "build:watch": "tsc --watch",
    "clean": "rimraf camelot-globe.{d.ts,d.ts.map,js,js.map} test/camelot-globe.{d.ts,d.ts.map,js,js.map} test/camelot-globe.spec.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat camelot-globe.bundled.js | gzip -9 | wc -c ; rm camelot-globe.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "dependencies": {},
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^3.0.0-next.1",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@types/three": "^0.138.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.3.9",
    "@web/test-runner": "^0.13.16",
    "@web/test-runner-mocha": "^0.7.4",
    "@web/test-runner-playwright": "^0.8.4",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "lit": "^2.1.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "three": "^0.138.3",
    "three-globe": "^2.24.3",
    "three-glow-mesh": "^0.1.2",
    "typescript": "^4.3.5"
  },
  "customElements": "custom-elements.json"
}
